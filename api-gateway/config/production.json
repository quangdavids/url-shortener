{
    "server": {
      "port": 4000,
      "host": "localhost"
    },
    "services": {
      "shortenURL": {
        "url": "http://localhost:5000",
        "timeout": 5000,
        "pathRewrite": {
          "/api/url": "/shorten"
        }
      },
      "redirectURL": {
        "url": "http://localhost:3000",
        "timeout": 5000,
        "pathRewrite": {
          "/api/redirect": "/api/redirect"
        }
      }
    },
    "rateLimit": {
      "windowMs": 900000,
      "max": 100
    },
    "cors": {
      "origin": "*",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    }
  }